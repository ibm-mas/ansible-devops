---
apiVersion: apps.mas.ibm.com/v1
kind: AiBrokerApp
metadata:
    name: "{{ mas_instance_id }}"
    namespace: "{{ aibroker_namespace }}"
    labels:
        mas.ibm.com/applicationId: aibroker
        mas.ibm.com/instanceId: "{{ mas_instance_id }}"
spec:
    settings:
        icr:
            cp: "{{ mas_icr_cp }}"
            cpopen: "{{ mas_icr_cpopen }}"
        sls:
            url: "{{ slscfg.url }}"
            registration_key: "{{ slscfg.registration_key }}"
            ca: "{{ slscfg.ca }}"
        dro:
            url: "{{ drocfg.url }}"
            registration_key: "{{ drocfg.registration_key }}"
            ca: "{{ drocfg.ca }}"
        jdbc:
            url: "{{ jdbccfg.url }}"
            username: "{{ jdbccfg.username }}"
            password: "{{ jdbccfg.password }}"
            ca: "{{ jdbccfg.ca }}"
