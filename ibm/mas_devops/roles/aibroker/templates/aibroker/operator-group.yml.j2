apiVersion: operators.coreos.com/v1
kind: OperatorGroup
metadata:
  name: mas-{{ mas_instance_id }}-aibroker-operator-group
  namespace: "{{ aibroker_namespace }}"
{% if custom_labels is defined and custom_labels.items() %}
  labels:
{% for key, value in custom_labels.items() %}
    "{{ key }}": "{{ value }}"
{% endfor %}
{% endif %}
spec:
  targetNamespaces:
    - "{{ aibroker_namespace }}"
