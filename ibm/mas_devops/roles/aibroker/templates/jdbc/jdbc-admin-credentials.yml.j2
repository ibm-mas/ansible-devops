---
kind: Secret
apiVersion: v1
metadata:
  name: "{{ mas_aibroker_db2_jdbc_secret }}"
  namespace: mas-{{ mas_instance_id }}-aibroker
data:
  username: "{{ jdbccfg.username | b64encode }}"
  password: "{{ jdbccfg.password | b64encode }}"
  url: "{{ jdbccfg.url }}"
  sslenabled: "{{ jdbccfg.sslenabled }}"
  certificate: "{{ jdbccfg.ca }}"
type: Opaque
