---
apiVersion: v1
kind: Service
metadata:
  name: minio-service
  namespace: {{ minio_namespace }}
spec:
  type: ClusterIP
  ports:
    - name: api
      port: 9000
      targetPort: 9000
      protocol: TCP
    - name: console
      port: 9090
      targetPort: 9090
      protocol: TCP
  selector:
    app: {{ minio_instance_name }}
