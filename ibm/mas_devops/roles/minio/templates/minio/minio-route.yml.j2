---
kind: Route
apiVersion: route.openshift.io/v1
metadata:
  name: minio-route
  namespace: "{{ minio_namespace }}"
spec:
  to:
    kind: Service
    name: minio-service
  port:
    targetPort: https
  tls:
    certificate: |
      {{ externalCert1 | indent(6, False) }}
    caCertificate: |
      {{ externalCert2 | indent(6, False) }}
    termination: reencrypt
    insecureEdgeTerminationPolicy: Redirect