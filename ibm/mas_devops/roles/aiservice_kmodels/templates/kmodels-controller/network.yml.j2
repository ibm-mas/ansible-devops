---
apiVersion: v1
kind: Service
metadata:
  name: km-controller
  namespace: aiservice-{{ aiservice_instance_id }}
spec:
  ports:
    - name: http
      port: 3000
      targetPort: http
    - name: https
      port: 8443
      targetPort: https
  selector:
    app: km-controller
  type: ClusterIP
